{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Node",
      "type": "pwa-node",
      "request": "attach",
      "restart": true,
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}/data/backDist",
      "remoteRoot": "/urs/src/app/dist",
      "outFiles": ["${workspaceFolder}/data/backDist/**/*.js"],
      "skipFiles": ["<node_internals>/**/*.js"],
      "protocol": "inspector",
      "sourceMaps": true
   },
  ]
}
